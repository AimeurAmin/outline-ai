{
  "name": "outline-ai",
  "version": "1.0.1",
  "description": "Q&A over your Outline wiki using embeddings and Claude",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "outline-ai": "./dist/cli.js"
  },
  "scripts": {
    "build": "bun run build:js && bun run build:types",
    "build:js": "bun build ./src/index.ts --outdir=dist --target=node && bun build ./src/cli.ts --outdir=dist --target=node --packages=external",
    "build:types": "tsc -p tsconfig.build.json",
    "dev": "bun run src/cli.ts",
    "index-docs": "bun run src/cli.ts index-docs",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "outline",
    "wiki",
    "embeddings",
    "claude",
    "anthropic",
    "qa"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AimeurAmin/outline-ai"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.76.0",
    "@xenova/transformers": "^2.17.2",
    "sharp": "^0.34.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5"
  }
}
